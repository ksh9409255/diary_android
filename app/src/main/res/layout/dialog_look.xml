<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="299dp"
    android:layout_height="171dp">

    <TextView
        android:id="@+id/dialog_look_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="삭제하시겠습니까?"
        android:textColor="#000000"
        android:fontFamily="@font/base_font"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4"/>

    <ImageButton
        android:id="@+id/dialog_btn_ok"
        android:layout_width="89dp"
        android:layout_height="26dp"
        android:background="@drawable/dialog_btn_ok"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/dialog_btn_no"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialog_look_text" />

    <ImageButton
        android:id="@+id/dialog_btn_no"
        android:layout_width="89dp"
        android:layout_height="26dp"
        android:background="@drawable/dialog_btn_no"
        android:text="아니요"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/dialog_btn_ok"
        app:layout_constraintTop_toBottomOf="@+id/dialog_look_text" />


</androidx.constraintlayout.widget.ConstraintLayout>